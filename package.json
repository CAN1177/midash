{
  "name": "midash",
  "description": "An alternative to lodash with the same API.",
  "version": "0.4.4",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "microbundle --compress false",
    "test": "tsdx test",
    "lint": "eslint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "shfshanyue",
  "repository": "shfshanyue/midash",
  "size-limit": [
    {
      "path": "dist/midash.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/midash.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "microbundle": "^0.15.1",
    "release-it": "^15.3.0",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "utils",
    "utilities",
    "toolkit",
    "pure",
    "functional"
  ]
}
