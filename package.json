{
  "name": "midash",
  "description": "An alternative to lodash with the same API.",
  "version": "0.2.8",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "microbundle",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "shfshanyue",
  "repository": "shfshanyue/midash",
  "size-limit": [
    {
      "path": "dist/midash.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/midash.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "microbundle": "^0.14.2",
    "release-it": "^14.14.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "utils",
    "utilities",
    "toolkit",
    "pure",
    "functional"
  ]
}
